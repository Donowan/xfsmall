/* 尺寸 */
.container{
    width:92%;
}
/* 文字 */
.delete{
    text-decoration: line-through
}
.header{
    height: 120rpx;
    /* line-height: 120rpx; */
}
.subHeader{
    height: 60rpx;
    line-height: 60rpx;
}
/* 文字******************************************** */
.textOver{
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-wrap: break-word;
    white-space: normal !important;
    -webkit-box-orient: vertical;
}
/* 单行省略
 */
.textOverOne{
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
/* 双行省略 */
.textOverTwo{
    -webkit-line-clamp: 2;
}
/* 三行省略 */
.textOverThree{
    -webkit-line-clamp: 3;
}
.miniSize{
    font-size: 18rpx
}


.VerticalNav.nav {
    width: 200rpx;
    white-space: initial;
  }
  
  .VerticalNav.nav .cu-item {
    width: 100%;
    text-align: center;
    background-color: #fff;
    margin: 0;
    border: none;
    height: 50px;
    position: relative;
  }
 
  .VerticalNav.nav .cu-item.cur {
    background-color: #f1f1f1;
  }
  .VerticalNav.nav .cu-item.cur::after {
    content: "";
    width: 8rpx;
    height: 30rpx;
    border-radius: 10rpx 0 0 10rpx;
    position: absolute;
    background-color: currentColor;
    top: 0;
    right: 0rpx;
    bottom: 0;
    margin: auto;
  }
  .VerticalBox{
    display: flex;
    border-top: 2rpx solid #ccc;
  }
  .VerticalMain{
    background-color: #f1f1f1;
  }

  /* 适应小屏幕 */
  .customImage{
    height: 140rpx !important;
    width: 140rpx !important;
  }
  .customRightContentHeight{
    height: 160rpx !important;
  }
  .cartEmpty .icon,.cartEmpty {
    width:calc(100vw - 200rpx) !important;
  }
  

.wrapSwiper{
    width:95vw;
}
.incomeBtn{
    position: fixed;
    z-index:1000;
    width:90rpx;
    height: 90rpx;
    bottom: 300rpx;
    right: 40rpx;
    animation: dh 1000ms ease-in-out infinite;
}
.income{
    font-size: 70rpx;
    border-radius: 12rpx;
    overflow: hidden;
}
@keyframes scale{
    0%{
        transform: scale(1)
    }
    50%{
        transform: scale(1.1)
    }
    100%{
        transform: scale(1)
    }
}
@keyframes dh {
    10% {
        transform: rotate(25deg);
        transform: scale(1)
    }

    20% {
        transform: rotate(-25deg);
    }

    30% {
        transform: rotate(25deg);
    }

    40% {
        transform: rotate(-25deg);
    }

    50% {
        transform: rotate(25deg);
        transform: scale(1.3)
    }

    60% {
        transform: rotate(-25deg);
    }

    70% {
        transform: rotate(25deg);
    }

    80% {
        transform: rotate(-25deg);
    }

    90% {
        transform: rotate(25deg);
    }

    100% {
        transform: rotate(-25deg);
        transform: scale(1)
    }
}
function KeepTwoDecimals(num) {

  var ten = Math.pow(10, 2);
  num = num * 1 + 0.0000001;
  var _num = ((Math.floor(num * ten)) / ten);


  return _num.toFixed(2)

}

function textOver(str) {
  var length = str.length;
  if (length > 70) {
    str = str.substring(0, 70) + ' ...';
  }
  return str
}

function rate(value) {
  //这里是图片的路径，自己需要改
  var data = {
    empty: 'data:image/png;base64,',//空
    half: 'data:image/png;base64,',//一半,
    full: 'data:image/png;base64,'//满　　　
  }
  var nums = []
  if ((value / 0.5) % 2 == 0) {
    for (var i = 0; i < 5; i++) {
      if (i < value) {
        console.log('1')
        nums.push({ src: data.full });
      } else {
        console.log('2')
        nums.push({ src: data.empty });
      }
    }
  } else {
    for (var i = 0; i < 5; i++) {
      if (i < value - 0.7) {
        console.log('3')
        nums.push({ src: data.full });
      } else if (i <= (value - 0.3) && i >= (value - 0.7)) {
        console.log('4')
        nums.push({ src: data.half });
      } else {
        console.log('5')
        nums.push({ src: data.empty });
      }
    }
  }
  return { array: nums };
}

function join(data, str) {
  return data.join(str)
}


module.exports = {
  KeepTwoDecimals: KeepTwoDecimals,
  rate: rate,
  join: join,
  textOver: textOver
}